{
  "name": "luiso-extension",
  "displayName": "luiso-extension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "luiso-extension.helloWorld",
        "title": "Hola mundo"
      },
      {
        "command": "luiso-extension.wordCount",
        "title": "Contar Palabras"
      },
      {
        "command": "luiso-extension.insertDate",
        "title": "Insertar Fecha"
      },
      {
        "command": "luiso-extension.selectTheme",
        "title": "Tema de Color"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "package-mainframe",
          "title": "nayeon mainframe",
          "icon": "resources/ico.png"
        }
      ]
    },
    "views": {
      "package-mainframe": [
          {
            "id": "package-dependencies-1",
            "name": "HOLA Lesly",
            "icon": "resources/ico.png"
          },
          {
            "id": "package-dependencies-2",
            "name": "Dependencias",
            "icon": "resources/ico.png"

          }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
