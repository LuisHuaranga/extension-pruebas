{
  "name": "luiso-extension",
  "displayName": "luiso-extension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "luiso-extension.helloWorld",
        "title": "Hola mundo"
      },
      {
        "command": "luiso-extension.wordCount",
        "title": "Contar Palabras"
      },
      {
        "command": "luiso-extension.insertDate",
        "title": "Insertar Fecha"
      },
      {
        "command": "luiso-extension.selectTheme",
        "title": "Tema de Color"
      },
      {
        "command": "luiso-extension.mensajeContextual",
        "title": "Convertir a mayusculas"
      },
      {
        "command": "luiso-extension.agregarElemento",
        "title": "‚ûï Agregar elemento",
        "category": "Mi Panel"
      },
      {
        "command": "luiso-extension.actualizarPanel",
        "title": "üîÑ Actualizar panel",
        "category": "Mi Panel"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "luiso-extension.mensajeContextual",
          "group": "navigation",
          "when": "editorHasSelection"
        },
        {
          "submenu": "luiso-extension/editorTools",
          "group": "modification",
          "when": "editorTextFocus"
        }
      ],
      "luiso-extension/editorTools": [
        {
          "command": "luiso-extension.helloWorld",
          "group": "0_actions"
        },
        {
          "command": "luiso-extension.wordCount",
          "group": "1_actions"
        },
        {
          "command": "luiso-extension.insertDate",
          "group": "1_actions"
        }
      ],
      "view/title": [
        {
          "command": "luiso-extension.actualizarPanel",
          "when": "view == luiso-extension.miPanel",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "luiso-extension.eliminarElemento",
          "when": "view == luiso-extension.miPanel"
        }
      ]
    },
    "submenus": [
      {
        "id": "luiso-extension/editorTools",
        "label": "üõ†Ô∏è Mis Herramientas",
        "icon": "$(tools)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "package-mainframe",
          "title": "nayeon mainframe",
          "icon": "resources/ico.png"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "luiso-extension.miPanel",
          "name": "üõ†Ô∏è Mi Panel Personalizado",
          "icon": "resources/ico.png"
        }
      ],
      "package-mainframe": [
        {
          "id": "package-dependencies-1",
          "name": "HOLA Lesly",
          "icon": "resources/ico.png"
        },
        {
          "id": "package-dependencies-2",
          "name": "Dependencias",
          "icon": "resources/ico.png"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}